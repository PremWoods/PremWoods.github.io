{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
  
   "title": {
    "text": "House value vs earnings",
    "subtitle":"Median house values compared to median UK salary, 1999-2021",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start"},
  
  
    "data": {
      "name":"myData",
      "url": "https://raw.githubusercontent.com/PremWoods/PremWoods.github.io/main/House%20price%20to%20earnings%20ratio.csv"},
            
     "repeat": {"layer":["Real Median house prices, Â£","Median Avg Salary UK"]},
  
       "spec": {
          
          "height": 400,
          "width": 400,
   
          "mark": {
              "type":"line",
              "strokeWidth":2},    
    
          "encoding": {
              "x": {
                  "field": "Year",
                  "type": "temporal"},
      
              "y": {
                  "field": {"repeat": "layer"},
                  "type": "quantitative",
                  "title": null},
      
              "color": {
                  "datum": {"repeat": "layer"},
                  "scale": {"range": ["black", "Red", "orange"]}}
            } 
        }
    }
  
  